{
  "name": "AUTOMATIONAPICYPRESS",
  "squad": "Esmeralda",
  "tipoTeste": "E2E - Aceitacao",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "cypress:open": "npx cypress open",
    "cypress": "npx cypress run --headless --browser=electron",
    "cypress:test": "npm run report:clean && npm run cypress",
    "cypress:report": "npm run report:merge && npm run report:mocha",
    "lint": "npx eslint ./cypress",
    "lint:fix": "npx eslint ./cypress --fix",
    "report:clean": "rm -rf mochawesome-report/* && rm -rf ${npm_package_name}",
    "report:merge": "mochawesome-merge > mochawesome-report/report.json && rm -rf mochawesome-report/*mochawesome*",
    "report:mocha": "marge -t 'ms-test-qa-automation-regression' -p report -f report --timestamp ddmmyyyy_HHMMss mochawesome-report/report.json",
    "report:slack": "cp -r mochawesome-report ${npm_package_name} && cypress-slack-reporter --vcs-provider none --ci-provider custom --custom-url https://cypress.ppay.me/ --report-dir ${npm_package_name} --custom-text \"$(node scripts/customTextReport.js)\"",
    "report:upload": "aws s3 sync ./mochawesome-report/ s3://test-cypress-report/${npm_package_name}",
    "start": "nodemon ./bin/server.js"
  },
  "author": "Skill QA",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tests-test-qa-automation-regression"
  },
  "bugs": {
    "url": "https://github.com/test/ms-test-qa-automation-regression/issues"
  },
  "homepage": "https://github.com/test/ms-test-qa-automation-regression#readme",
  "dependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-xpath": "^1.4.0",
    "debug": "4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "faker": "^6.6.6",
    "faker-br": "^0.4.1",
    "fs-extra": "^9.0.1",
    "globby": "11.1.0",
    "joi": "^10.4.1",
    "joi-to-json-schema": "^5.1.0",
    "jose": "^2.0.5",
    "ls": "^0.2.1",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.1",
    "ncp": "^2.0.0",
    "node": "^21.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^2.2.4",
    "@pedro.pires/cypress-slack-reporter": "^1.2.1",
    "@shelex/cypress-allure-plugin": "^2.26.5",
    "@slack/web-api": "^6.8.1",
    "@types/mocha": "7.0.2",
    "chance": "^1.0.18",
    "cypress": "^13.6.2",
    "cypress-localstorage-commands": "^2.2.4",
    "date-fns": "^2.18.0",
    "eslint": "^8.41.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tv4": "^1.3.0"
  }
